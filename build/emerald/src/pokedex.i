static void ResetOtherVideoRegisters(u16 a)
{
    if (!(a & 0x0100))
    {
        ClearGpuRegBits(0, 0x0100);
        SetGpuReg(0x8, 0);
        SetGpuReg(0x10, 0);
        SetGpuReg(0x12, 0);
    }
    if (!(a & 0x0200))
    {
        ClearGpuRegBits(0, 0x0200);
        SetGpuReg(0xa, 0);
        SetGpuReg(0x14, 0);
        SetGpuReg(0x16, 0);
    }
    if (!(a & 0x0400))
    {
        ClearGpuRegBits(0, 0x0400);
        SetGpuReg(0xc, 0);
        SetGpuReg(0x18, 0);
        SetGpuReg(0x1a, 0);
    }
    if (!(a & 0x0800))
    {
        ClearGpuRegBits(0, 0x0800);
        SetGpuReg(0xe, 0);
        SetGpuReg(0x1c, 0);
        SetGpuReg(0x1e, 0);
    }
    if (!(a & 0x1000))
    {
        ClearGpuRegBits(0, 0x1000);
        ResetSpriteData();
        FreeAllSpritePalettes();
        gReservedSpritePaletteCount = 8;
    }
}


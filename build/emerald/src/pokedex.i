        }
        if ((gMain.newKeys & 0x0010)
         || ((gMain.newKeys & 0x0100) && gSaveBlock2Ptr->optionsButtonMode == 1))
        {
            if (!sPokedexListItem->owned)
            {
                PlaySE(32);
            }
            else
            {
                BeginNormalPaletteFade(0xFFFFFFEB, 0, 0, 0x10, ((0) | ((0) << 5) | ((0) << 10)));

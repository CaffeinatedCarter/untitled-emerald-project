static u16 GetNextPosition(u8 direction, u16 position, u16 min, u16 max)
{
    switch (direction)
    {
    case 1:
        if (position > min)
            position--;
        break;
    case 0:
        if (position < max)
            position++;
        break;
    case 3:
        if (position > min)
            position--;
        else
            position = max;
        break;
    case 2:
        if (position < max)
            position++;
        else
            position = min;
        break;
    }
    return position;
}




        gBattleTextBuff1[i] = gBattleMsgDataPtr->textBuffs[0][i];
        gBattleTextBuff2[i] = gBattleMsgDataPtr->textBuffs[1][i];
        gBattleTextBuff3[i] = gBattleMsgDataPtr->textBuffs[2][i];
    }

    switch (stringID)
    {
    case 0:
        if (gBattleTypeFlags & 0x0008)
        {
            if (gBattleTypeFlags & (0x0002 | 0x2000000))
            {
                if (gBattleTypeFlags & 0x800000)
                {
                    stringPtr = sText_TwoTrainersWantToBattle;
                }
                else if (gBattleTypeFlags & 0x0040)
                {
                    if (gBattleTypeFlags & 0x1000000)
                        stringPtr = sText_TwoLinkTrainersWantToBattlePause;
                    else
                        stringPtr = sText_TwoLinkTrainersWantToBattle;
                }
                else
                {
                    if (gTrainerBattleOpponent_A == 3072)
                        stringPtr = sText_Trainer1WantsToBattle;
                    else if (gBattleTypeFlags & 0x1000000)
                        stringPtr = sText_LinkTrainerWantsToBattlePause;
                    else
                        stringPtr = sText_LinkTrainerWantsToBattle;
                }
            }
            else
            {
                if (gBattleTypeFlags & 0x400000)
                    stringPtr = sText_TwoTrainersWantToBattle;
                else if (gBattleTypeFlags & 0x8000)
                    stringPtr = sText_TwoTrainersWantToBattle;
                else
                    stringPtr = sText_Trainer1WantsToBattle;
            }
        }
        else
        {
            if (gBattleTypeFlags & 0x2000)
                stringPtr = sText_WildPkmnAppeared2;
            else if (gBattleTypeFlags & 0x0001)
                stringPtr = sText_TwoWildPkmnAppeared;
            else if (gBattleTypeFlags & 0x0200)
                stringPtr = sText_WildPkmnAppearedPause;
            else
                stringPtr = sText_WildPkmnAppeared;
        }
        break;
    case 1:
        if (GetBattlerSide(gActiveBattler) == 0)
        {
            if (gBattleTypeFlags & 0x0001)
            {
                if (gBattleTypeFlags & 0x400000)
                    stringPtr = sText_InGamePartnerSentOutZGoN;
                else if (gBattleTypeFlags & 0x8000)
                    stringPtr = sText_GoTwoPkmn;
                else if (gBattleTypeFlags & 0x0040)
                    stringPtr = sText_LinkPartnerSentOutPkmnGoPkmn;
                else
                    stringPtr = sText_GoTwoPkmn;
            }
            else
            {
                stringPtr = sText_GoPkmn;
            }
        }
        else
        {
            if (gBattleTypeFlags & 0x0001)
            {
                if (gBattleTypeFlags & 0x8000)
                    stringPtr = sText_TwoTrainersSentPkmn;
                else if (gBattleTypeFlags & 0x800000)
                    stringPtr = sText_TwoTrainersSentPkmn;
                else if (gBattleTypeFlags & 0x0040)
                    stringPtr = sText_TwoLinkTrainersSentOutPkmn;
                else if (gBattleTypeFlags & (0x0002 | 0x2000000))
                    stringPtr = sText_LinkTrainerSentOutTwoPkmn;
                else
                    stringPtr = sText_Trainer1SentOutTwoPkmn;
            }
            else
            {
                if (!(gBattleTypeFlags & (0x0002 | 0x2000000)))
                    stringPtr = sText_Trainer1SentOutPkmn;
                else if (gTrainerBattleOpponent_A == 3072)
                    stringPtr = sText_Trainer1SentOutPkmn;
                else
                    stringPtr = sText_LinkTrainerSentOutPkmn;
            }
        }
        break;
    case 2:
        if (GetBattlerSide(gActiveBattler) == 0)
        {
            if (*(&gBattleStruct->hpScale) == 0)
                stringPtr = sText_PkmnThatsEnough;
            else if (*(&gBattleStruct->hpScale) == 1 || gBattleTypeFlags & 0x0001)
                stringPtr = sText_PkmnComeBack;
            else if (*(&gBattleStruct->hpScale) == 2)
                stringPtr = sText_PkmnOkComeBack;
            else
                stringPtr = sText_PkmnGoodComeBack;
        }
        else
        {
            if (gTrainerBattleOpponent_A == 2048 || gBattleTypeFlags & 0x2000000)
            {
                if (gBattleTypeFlags & 0x0040)
                    stringPtr = sText_LinkTrainer2WithdrewPkmn;
                else
                    stringPtr = sText_LinkTrainer1WithdrewPkmn;
            }
            else
            {
                stringPtr = sText_Trainer1WithdrewPkmn;
            }
        }
        break;
    case 3:
        if (GetBattlerSide(gBattleScripting.battler) == 0)
        {
            if (*(&gBattleStruct->hpScale) == 0 || gBattleTypeFlags & 0x0001)
                stringPtr = sText_GoPkmn2;
            else if (*(&gBattleStruct->hpScale) == 1)
                stringPtr = sText_DoItPkmn;
            else if (*(&gBattleStruct->hpScale) == 2)
                stringPtr = sText_GoForItPkmn;
            else
                stringPtr = sText_YourFoesWeakGetEmPkmn;
        }
        else
        {
            if (gBattleTypeFlags & (0x0002 | 0x2000000))
            {
                if (gBattleTypeFlags & 0x800000)
                {
                    if (gBattleScripting.battler == 1)
                        stringPtr = sText_Trainer1SentOutPkmn2;
                    else
                        stringPtr = sText_Trainer2SentOutPkmn;
                }
                else
                {
                    if (gBattleTypeFlags & 0x0040)
                        stringPtr = sText_LinkTrainerMultiSentOutPkmn;
                    else if (gTrainerBattleOpponent_A == 3072)
                        stringPtr = sText_Trainer1SentOutPkmn2;
                    else
                        stringPtr = sText_LinkTrainerSentOutPkmn2;
                }
            }
            else
            {
                if (gBattleTypeFlags & 0x8000)
                {
                    if (gBattleScripting.battler == 1)
                        stringPtr = sText_Trainer1SentOutPkmn2;
                    else
                        stringPtr = sText_Trainer2SentOutPkmn;
                }
                else
                {
                    stringPtr = sText_Trainer1SentOutPkmn2;
                }
            }
        }
        break;
    case 4:
        ChooseMoveUsedParticle(gBattleTextBuff1);

        if (gBattleMsgDataPtr->currentMove >= 355)
            StringCopy(gBattleTextBuff2, sATypeMove_Table[*(&gBattleStruct->stringMoveType)]);
        else
            StringCopy(gBattleTextBuff2, gMoveNames[gBattleMsgDataPtr->currentMove]);

        ChooseTypeOfMoveUsedString(gBattleTextBuff2);
        stringPtr = sText_AttackerUsedX;
        break;
    case 5:
        if (gBattleTextBuff1[0] & 0x80)
        {
            gBattleTextBuff1[0] &= ~(0x80);
            if (GetBattlerSide(gActiveBattler) == 1 && gBattleTextBuff1[0] != 0x3)
                gBattleTextBuff1[0] ^= (0x2 | 0x1);

            if (gBattleTextBuff1[0] == 0x2 || gBattleTextBuff1[0] == 0x3)
                stringPtr = sText_GotAwaySafely;
            else if (gBattleTypeFlags & 0x0040)
                stringPtr = sText_TwoWildFled;
            else
                stringPtr = sText_WildFled;
        }
        else
        {
            if (GetBattlerSide(gActiveBattler) == 1 && gBattleTextBuff1[0] != 0x3)
                gBattleTextBuff1[0] ^= (0x2 | 0x1);

            if (gBattleTypeFlags & 0x0040)
            {
                switch (gBattleTextBuff1[0])
                {
                case 0x1:
                    if (gBattleTypeFlags & 0x800000)
                        stringPtr = sText_TwoInGameTrainersDefeated;
                    else
                        stringPtr = sText_TwoLinkTrainersDefeated;
                    break;
                case 0x2:
                    stringPtr = sText_PlayerLostToTwo;
                    break;
                case 0x3:
                    stringPtr = sText_PlayerBattledToDrawVsTwo;
                    break;
                }
            }
            else if (gTrainerBattleOpponent_A == 3072)
            {
                switch (gBattleTextBuff1[0])
                {
                case 0x1:
                    stringPtr = sText_PlayerDefeatedLinkTrainerTrainer1;
                    break;
                case 0x2:
                    stringPtr = sText_PlayerLostAgainstTrainer1;
                    break;
                case 0x3:
                    stringPtr = sText_PlayerBattledToDrawTrainer1;
                    break;
                }
            }
            else
            {
                switch (gBattleTextBuff1[0])
                {
                case 0x1:
                    stringPtr = sText_PlayerDefeatedLinkTrainer;
                    break;
                case 0x2:
                    stringPtr = sText_PlayerLostAgainstLinkTrainer;
                    break;
                case 0x3:
                    stringPtr = sText_PlayerBattledToDrawLinkTrainer;
                    break;
                }
            }
        }
        break;
    default:
        if (stringID >= 369 + 12)
        {
            gDisplayedStringBattle[0] = 0xFF;
            return;
        }
        else
        {
            stringPtr = gBattleStringsTable[stringID - 12];
        }
        break;
    }

    BattleStringExpandPlaceholdersToDisplayedString(stringPtr);
}

